{
  "name": "mocha-extjs",
  "version": "0.1.6",
  "description": "ExtJs Test Framework for Mocha",
  "main": "mocha-extjs.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "./node_modules/.bin/gulp",
    "test": "./node_modules/mocha-phantomjs/bin/mocha-phantomjs --timeout 15000 --path ./node_modules/phantomjs/bin/phantomjs --view 1024x768 http://localhost:3000",
    "lint": "./node_modules/.bin/standard gulpfile.js mocha-extjs.js ./src/**/*.js ./test/suites/**/*.js ./test/sandbox/app/**/*.js | ./node_modules/.bin/snazzy",
    "copy-demo": "./scripts/copy-demo.sh"
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "expect",
      "eTT",
      "Ext",
      "Sandbox"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/antonfisher/node-mocha-extjs.git"
  },
  "keywords": [
    "extjs",
    "sencha",
    "mocha",
    "test framework"
  ],
  "author": "Anton Fisher <a.fschr@gmail.com> (http://antonfisher.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/antonfisher/node-mocha-extjs/issues"
  },
  "homepage": "https://github.com/antonfisher/node-mocha-extjs",
  "devDependencies": {
    "babel-core": "6.x",
    "babel-loader": "6.x",
    "babel-polyfill": "6.7.4",
    "babel-preset-es2015": "6.x",
    "browser-sync": "2.x",
    "gulp": "3.x",
    "gulp-sass": "2.x",
    "gulp-sourcemaps": "1.x",
    "mocha-phantomjs": "4.0.2",
    "snazzy": "3.x",
    "standard": "6.x",
    "webpack-stream": "3.x"
  }
}
