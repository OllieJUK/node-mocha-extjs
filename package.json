{
  "name": "mocha-extjs",
  "version": "0.1.1",
  "description": "ExtJs Test Framework for Mocha",
  "main": "mocha-extjs.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "./node_modules/.bin/gulp",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "./node_modules/.bin/standard mocha-extjs.js gulpfile.js ./src/**/*.js ./test/suites/**/*.js ./test/sandbox/app/**/*.js | ./node_modules/.bin/snazzy"
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "expect",
      "eTT",
      "Ext",
      "Sandbox"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/antonfisher/mocha-extjs.git"
  },
  "keywords": [
    "extjs",
    "sencha",
    "mocha",
    "test framework"
  ],
  "author": "Anton Fisher <a.fschr@gmail.com> (http://antonfisher.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/antonfisher/extjs-testing-tool/issues"
  },
  "homepage": "https://github.com/antonfisher/extjs-testing-tool",
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "browser-sync": "^2.11.1",
    "gulp": "^3.9.0",
    "gulp-sass": "^2.1.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-webpack": "^1.5.0",
    "snazzy": "^2.0.1",
    "standard": "^5.4.1"
  }
}
